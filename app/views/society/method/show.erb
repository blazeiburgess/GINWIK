<h1><%= @society.name %></h1>
<h3><%= method_name_to_plain_language(params[:method]) %></h3>
<% if current_user(session) == @society.user %>
  <a href="/societies/<%= get_slug(@society) %>/<%= params[:method] %>/edit"><button>Edit entry</button></a>
<% end %>
<hr>
<% @method.attribute_names.each do |attribute| %>
  <% if !attribute.include?("id") %>
	<h5><%= method_name_to_plain_language(attribute) %></h5>
    <div class="row">
	<div class="four columns"></div>
	<div class="eight columns">
	  <p><%= @method.send(attribute.to_sym) %></p>
	</div>
    </div>
  <% end %>
<% end %>
<br>
<a href="/societies/<%= get_slug(@society) %>">Go Back</a>
